{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:56482", // URL base para IIS Express
      "sslPort": 44350 // Puerto SSL para IIS Express
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger", // Página que se abrirá en el navegador al lanzar la aplicación
      "applicationUrl": "http://localhost:6000", // URL base para la aplicación HTTP
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger", // Página que se abrirá en el navegador al lanzar la aplicación
      "applicationUrl": "https://localhost:8150;http://localhost:6100", // URLs base para la aplicación HTTPS y HTTP
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger", // Página que se abrirá en el navegador al lanzar la aplicación con IIS Express
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
